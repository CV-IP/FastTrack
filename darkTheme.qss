QProgressBar {
    background-color: black;
    border: 1px solid white;
	border-radius: 10px;
    text-align: center;
    color: white;
    border-radius: 10px;
}

QProgressBar::chunk {
    background-color: orange;
    width: 10px;
    margin: 0.5px;
    border-radius: 10px;

}


QSlider::groove:horizontal {
    border: 1px solid black;
    height: 8px;
    background: lightgrey;
    margin: 2px 0;
}

QSlider::handle:horizontal {
    background: white;
    border: 10px black;
    width: 18px;
    margin: -2px 0;
    border-radius: 3px;
}

