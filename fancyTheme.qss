QProgressBar {
    background-color: #ff5107;
    border: 1px solid #003091;
    border-radius: 10px;
    text-align: center;
    color: #003091;
    border-radius: 10px;
}

QProgressBar::chunk {
    background-color: orange;
    width: 10px;
    margin: 0.5px;
    border-radius: 10px;

}


QSlider::groove:horizontal {
    border: 1px solid #ff5107;
    height: 8px;
    background: #003091;
    margin: 2px 0;
}

QSlider::handle:horizontal {
    background: #003091;
    width: 18px;
    margin: -2px 0;
    border-radius: 3px;
}

QWidget#central {
    background: #ff5107;
}


